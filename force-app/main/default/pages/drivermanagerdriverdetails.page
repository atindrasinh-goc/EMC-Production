                        {
                            $scope.empContact[0].Car_Image__c = '';
                        }
                        if($scope.empContact[0].Account.Logo_Document_Id__c == null || $scope.empContact[0].Account.Logo_Document_Id__c == '' || $scope.empContact[0].Account.Logo_Document_Id__c == undefined){
